CREATE TABLE IF NOT EXISTS `mydb`.`registro_transacoes` (\
  `id` INT NOT NULL AUTO_INCREMENT,\
  `data` DATE NOT NULL,\
  `tipo` VARCHAR(15) NOT NULL,\
  `valor` DECIMAL(2) NOT NULL,\
  `nome_favorecido` VARCHAR(90) NOT NULL,\
  `conta_favorecido` VARCHAR(45) NOT NULL,\
  `ag_favorecido` VARCHAR(45) NOT NULL,\
  `banco_favorecido` VARCHAR(45) NOT NULL,\
  PRIMARY KEY (`id`))\
ENGINE = InnoDB}
